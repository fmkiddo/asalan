{
	"name" : "fmkiddo/asalan",
	"description" : "Asset Management System by FMKiddo",
	"license" : "GPL-3.0",
	"type" : "project",
	"homepage" : "https://gitlab.com/fmkiddo/asalan",
	"support" : {
		"source" : "https://gitlab.com/fmkiddo/asalan",
		"slack" : "https://codeigniterchat.slack.com",
		"email" : "rizkyfm64@gmail.com"
	},
	"require" : {
		"php": "^8.1",
		"codeigniter4/framework": "4.5.5",
		"dompdf/dompdf": "^3.1",
		"phpoffice/phpspreadsheet": "^3.9"
	},
	"require-dev" : {
		"fakerphp/faker" : "^1.9",
		"mikey179/vfsstream" : "^1.6",
		"phpunit/phpunit" : "^10.5.16"
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "app/",
			"Config\\" : "app/Config/"
		},
		"exclude-from-classmap" : [
			"**/Database/Migrations/**"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\Support\\" : "tests/_support"
		}
	},
	"config" : {
		"optimize-autoloader" : true,
		"preferred-install" : "dist",
		"sort-packages" : true
	},
	"scripts" : {
		"test" : "phpunit"
	},
	"authors" : [{
			"name" : "fmkiddo",
			"email" : "rizkyfm64@gmail.com",
			"homepage" : "https://gitlab.com/fmkiddo/asalan"
		}
	]
}
